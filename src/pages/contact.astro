---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---

<Layout title="Felipe Franco Artist Contact" />

<Header />

<main
    class="flex flex-col place-items-start w-[90%] max-w-[1000px] gap-6 mt-10"
>
    <h1 class="text-xl">Contact</h1>
    <div class="flex place-items-center gap-5">
        <img
            src="data:image/png;base64,"
        />
        <p>lfelipe.alzate1@gmail.com</p>
    </div>

    <div class="flex place-items-center gap-5">
        <img
            src="data:image/png;base64,"
        />
        <a target="_blank" href="https://www.instagram.com/felipe.afranco/">
            <p>@felipe.afranco</p>
        </a>
    </div>
</main>
